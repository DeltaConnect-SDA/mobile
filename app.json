{
  "expo": {
    "name": "DeltaConnect",
    "slug": "deltaconnect",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#219653"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.FOREGROUND_SERVICE"
      ],
      "package": "com.navaweb.deltaconnect"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "notification": {
      "icon": "./assets/notif-icon.png",
      "color": "#FFFFFF"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Izinkan $(PRODUCT_NAME) untuk mengakses kamera."
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Izin $(PRODUCT_NAME) untuk mengakses lokasi anda."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/notif-icon.png",
          "color": "#ffffff"
        }
      ],
      "react-native-compressor"
    ],
    "extra": {
      "eas": {
        "projectId": "5f0d2680-2056-4bde-b0e8-bbbdb09c310b"
      }
    }
  }
}